    <string name="rec_btn_off">Stop rec</string>
    <string name="slider_cutoff">Bass filter cutoff</string>
    <string name="slider_decay">Bass synth decay</string>
    <string name="slider_mix">Chord delay mix</string>
    <string name="slider_pitch">Drum pitch</string>
    <string name="slider_tempo">Sequencer tempo</string>
    <string name="slider_volume">Output volume</string>
